  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "eslint ./src --color",
    "lint:fix": "eslint ./src --fix --color",
    "format": "prettier --write ./src/**/*.{js,jsx}"
